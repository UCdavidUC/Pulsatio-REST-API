<!DOCTYPE html>
<html>
<head>
    <script src="https://code.highcharts.com/highcharts.js"></script>
    <script src="https://code.highcharts.com/highcharts-more.js"></script>
    <script src="https://code.highcharts.com/modules/exporting.js"></script>
</head>
<body>
    <div id="container" style="min-width: 310px; height: 400px; margin: 0 auto"></div>
    <script>

    var ranges = <%- JSON.stringify(rng) %>;
    var averages = <%- JSON.stringify(avg) %>;

    console.log(ranges);
    console.log(averages)


        Highcharts.chart('container', {

            title: {
                text: 'Ritmo Cardiaco'
            },

            xAxis: {
                type: 'datetime'
            },

            yAxis: {
                title: {
                    text: null
                }
            },

            tooltip: {
                crosshairs: true,
                shared: true,
                valueSuffix: ' lpm'
            },

            legend: {
            },

            series: [{
                name: 'Frecuencia',
                data: averages,
                zIndex: 1,
                color: 'red',
                marker: {
                    fillColor: 'white',
                    lineWidth: 2,
                    lineColor: '#FF323A'
                }
            }, {
                name: 'Frecuencia min/max',
                data: ranges,
                type: 'arearange',
                lineWidth: 0,
                linkedTo: ':previous',
                color: '#FF323A',
                fillOpacity: 0.3,
                zIndex: 0
            }]
        });
    </script>
</body>
</html>